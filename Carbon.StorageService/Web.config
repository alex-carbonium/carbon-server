<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>
  <appSettings file="SharedAppSettings.config">
    <add key="ProjectDataPath" value="/RestrictedContent/Projects"/>
    <add key="aspnet:MaxJsonDeserializerMembers" value="10000000"/>
    <add key="storageUrl" value="http://127.0.0.1:10000/ "/>
  </appSettings>
  <!--  In debug mode, connection strings are copied in a build event-->
  <connectionStrings configSource="ConnectionStrings.config"/>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1" maxRequestLength="6000000"/>
    <customErrors mode="Off"/>
  </system.web>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Access-Control-Allow-Origin" value="*"/>
        <add name="Access-Control-Allow-Headers" value="Content-Type,X-Requested-With,Authorization,X-SessionId,Cache-Control,Origin,Accept,X-Accept"/>
        <add name="Access-Control-Allow-Methods" value="GET,POST,PUT,DELETE,OPTIONS"/>
      </customHeaders>
    </httpProtocol>
    <modules runAllManagedModulesForAllRequests="true"/>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
    </handlers>
  </system.webServer>
  <log4net>
    <!--      <appender name="NHibernate.SQL" type="log4net.Appender.TraceAppender">-->
    <!--        <layout type="log4net.Layout.PatternLayout">-->
    <!--          <conversionPattern value="%date %-5level - %message%newline" />-->
    <!--        </layout>-->
    <!--      </appender>-->
    <!--      <logger name="NHibernate.SQL" additivity="false">-->
    <!--        <level value="DEBUG" />-->
    <!--        <appender-ref ref="NHibernate.SQL" />-->
    <!--      </logger>-->
  </log4net>
  <location path="Logs">
    <system.web>
      <authorization>
        <allow roles="Administrators"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
